@model List<LendingLibrary.Web.ViewModels.ItemViewModel>


@{
    ViewBag.Title = "title";
}


<table>
    <thead>
    <th>
        Item Name
    </th>
    <th>
        Person Name
    </th>
    <th>
        Days Lent Out
    </th>
    </thead>

    @foreach (var item in Model)
    {

        <tr>
            <td>@item.ItemName</td>
            <td>@item.PersonName</td>
            <td>@item.DaysLentOut</td>
        </tr>
    }
</table>

